<Window x:Class="Just_Do_It_Rabbit_12_Explosion.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Just_Do_It_Rabbit_12_Explosion"
        mc:Ignorable="d"
        Title="MainWindow" Height="auto" Width="auto">
    <Canvas HorizontalAlignment="Right" Width="760">
        <TextBox x:Name="Name" Canvas.Top="40" Canvas.Left="170">Rabbit Name</TextBox>
        <TextBox x:Name="Age" Canvas.Top="40" Canvas.Left="270">Rabbit Age</TextBox>
        <Button Canvas.Top="40" Canvas.Left="470" Click="Button_Click">Add Rabbit</Button>
        <ListBox x:Name="ListBox1"></ListBox>
        <TextBlock Name="Tb1" Canvas.Bottom="50" Canvas.Left="170"></TextBlock>
        <TextBlock Name="Tb2" Canvas.Bottom="50" Canvas.Left="270"></TextBlock>
        <Image x:Name="rabbet" Canvas.Bottom="250" Canvas.Left="170" Height="226" Canvas.Top="84" Width="364"></Image>
    </Canvas>
</Window>
