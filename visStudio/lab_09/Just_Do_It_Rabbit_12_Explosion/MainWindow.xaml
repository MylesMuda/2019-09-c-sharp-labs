<Window x:Class="Just_Do_It_Rabbit_12_Explosion.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Just_Do_It_Rabbit_12_Explosion"
        mc:Ignorable="d"
        Title="MainWindow" Height="auto" Width="auto">
    <Canvas>
        <TextBox Canvas.Top="40" Canvas.Left="170">Rabbit Name</TextBox>
        <TextBox Canvas.Top="40" Canvas.Left="270">Rabbit Age</TextBox>
        <Button Canvas.Top="40" Canvas.Left="470" Click="Button_Click">Add Rabbit</Button>
    </Canvas>
</Window>
